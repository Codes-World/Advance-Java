<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="2.4" xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
	<display-name>ServletApp-06</display-name>
	
	<!-- Servlet Context Data -->
	<context-param>
		<param-name>username</param-name>
		<param-value>aman</param-value>
	</context-param>
	
	<context-param>
		<param-name>password</param-name>
		<param-value>aman@123</param-value>
	</context-param>
	
	<context-param>
		<param-name>email</param-name>
		<param-value>aman@gmail.com</param-value>
	</context-param>
	
	<servlet>
		<servlet-name>DemoServlet</servlet-name>
		<servlet-class>in.codeworld.aman.controller.FirstServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>DemoServlet</servlet-name>
		<url-pattern>/demo</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>HelloServlet</servlet-name>
		<servlet-class>in.codeworld.aman.controller.SecondServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>HelloServlet</servlet-name>
		<url-pattern>/test</url-pattern>
	</servlet-mapping>
	
</web-app>
